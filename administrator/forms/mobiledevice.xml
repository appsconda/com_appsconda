<?xml version="1.0" encoding="utf-8"?>
<form addfieldprefix="Joomla\Component\Appsconda\Administrator\Field">
	<fieldset name="details">

       <field name="id" type="text" default="0" label="COM_APPSCONDA_FORM_LBL_MOBILEDEVICE_ID"
            readonly="true" class="readonly"
            description="JGLOBAL_FIELD_ID_DESC" /> 

	   <field
			name="juserid"
			type="text"
			label="COM_APPSCONDA_MOBILEDEVICE_JUSERID_LBL"
			hint="COM_APPSCONDA_MOBILEDEVICE_JUSERID_DESC"
			readonly="true"
			required="true"
			default=""
			/>
	
		<field
			name="push"
			type="list"
			label="COM_APPSCONDA_MOBILEDEVICE_PUSH_LBL"
			hint="COM_APPSCONDA_MOBILEDEVICE_PUSH_DESC"
			class="inputbox"
			size="1"
			readonly="true"
			required="true"
			default=""
			>
			<option value="true">Yes</option>
			<option value="false">No</option>
		</field>
	
		<field
			name="created_time"
			type="text"
			label="COM_APPSCONDA_MOBILEDEVICE_CREATED_TIME_LBL"
			hint="COM_APPSCONDA_MOBILEDEVICE_CREATED_TIME_DESC"
			readonly="true"
			required="true"
			default=""
			/>
	
		<field
			name="modified_time"
			type="text"
			label="COM_APPSCONDA_MOBILEDEVICE_MODIFIED_TIME_LBL"
			hint="COM_APPSCONDA_MOBILEDEVICE_MODIFIED_TIME_DESC"
			readonly="true"
			required="true"
			default=""
			/>
	
		<field
			name="devicetoken"
			type="textarea"
			label="COM_APPSCONDA_MOBILEDEVICE_DEVICETOKEN_LBL"
			hint="COM_APPSCONDA_MOBILEDEVICE_DEVICETOKEN_DESC"
			filter="raw"
			readonly="true"
			required="true"
			default=""
			/>
	
		<field
			name="created_by"
			type="createdby"
			label="COM_APPSCONDA_MOBILEDEVICE_CREATED_BY_LBL"
			hint="COM_APPSCONDA_MOBILEDEVICE_CREATED_BY_DESC"
			default=""
			/>
	
		<field
			name="state"
			type="list"
			label="COM_APPSCONDA_MOBILEDEVICE_STATE_LBL"
			hint="COM_APPSCONDA_MOBILEDEVICE_STATE_DESC"
			class="inputbox"
			size="1"
			default="1"
			>
			<option value="1">JPUBLISHED</option>
			<option value="0">JUNPUBLISHED</option>
			<option value="2">JARCHIVED</option>
			<option value="-2">JTRASHED</option>
		</field>
	
		<field
			name="ordering"
			type="hidden"
			label="COM_APPSCONDA_MOBILEDEVICE_ORDERING_LBL"
			hint="COM_APPSCONDA_MOBILEDEVICE_ORDERING_DESC"
			default="0"
			/>

	</fieldset>
</form>
